{
  "name": "agreport-sdk",
  "version": "0.0.1",
  "description": "a report sdk",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "unpkg": "dist/index.js",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:cjs build:mjs build:umd",
    "build:cjs": "rollup --config ./scripts/rollup-cjs.config.js && run-s entry:cjs",
    "build:mjs": "rollup --config ./scripts/rollup-mjs.config.js && run-s entry:mjs",
    "build:umd": "rollup --config ./scripts/rollup-umd.config.js",
    "build:docs":"",
    "playground:build":"playground:* ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./dist ./lib ./es",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "devDependencies":{
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.16.11",
    "prettier": "^2.7.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "4.1.5",
    "typescript": "4.5.2",
    "yorkie": "^2.0.0",
    "rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "bumpp": "^8.2.1",
    "auto-changelog": "^2.4.0"
  },
  "dependencies":{},
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
